+    except Exception as e:
+        logger.error(
+            "Failed to get system status",
+            extra={
+                "correlation_id": correlation_id,
+                "error": str(e)
+            }
+        )
+        raise HTTPException(
+            status_code=500,
+            detail=f"Failed to retrieve system status: {str(e)}"
+        )
